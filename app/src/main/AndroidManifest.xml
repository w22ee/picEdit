        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".EditTagActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustResize|stateUnchanged">
